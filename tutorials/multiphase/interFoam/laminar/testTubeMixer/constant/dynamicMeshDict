// Tube rocking on rotating table
rotatingBox
{
    solidBodyMotionFunction oscillatingRotatingMotion;
    oscillatingRotatingMotionCoeffs
    {
        origin          (0 0 0);
        omega           40;         // rad/s
        amplitude       (45 0 0);   // 45 degrees max tilt
    }
}



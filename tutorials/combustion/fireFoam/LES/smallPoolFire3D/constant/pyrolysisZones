FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "constant";
    object      pyrolysisZones;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pyrolysis
{
    active          false;

    pyrolysisModel  none;

    regionName      panelRegion;

    reactingOneDimCoeffs
    {
        radFluxName     qr;

        minimumDelta    1e-8;

        moveMesh        false;
    }

    infoOutput      false;
}


